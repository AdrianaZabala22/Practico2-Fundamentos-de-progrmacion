#include <stdio.h>
int main() {
    int n, i;
    long long fact = 1;
    printf("Ingrese N: ");
    scanf("%d", &n);
    if(n < 0) {
        printf("No existe factorial de negativo\n");
    } else {
        for(i = 1; i <= n; i++) {
            fact *= i;
        }
        printf("%d! = %lld\n", n, fact);
    }
    return 0;
}
