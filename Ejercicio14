#include <stdio.h>
int main() {
    int n, i;
    long long a = 1, b = 1, c;
    printf("Posicion: ");
    scanf("%d", &n);
    if(n <= 0) {
        printf("Posicion invalida\n");
        return 0;
    }
    if(n == 1 || n == 2) {
        printf("F(%d) = 1\n", n);
        return 0;
    }
    for(i = 3; i <= n; i++) {
        c = a + b;
        a = b;
        b = c;
    }
    printf("F(%d) = %lld\n", n, b);
    return 0;
}
